<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
>


    <TextView
            android:id="@+id/textView10"
            android:layout_width="105dp"
            android:layout_height="24dp"
            android:text="Location:"
            android:textColor="#787676"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.205"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.71"/>
    <TextView
            android:id="@+id/locationTextView"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:text="TextView"
            android:textColor="#FAF9F9"
            android:textSize="16sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.816"
            app:layout_constraintVertical_bias="0.712"/>
    <TextView
            android:id="@+id/role"
            android:layout_width="271dp"
            android:layout_height="36dp"
            android:layout_marginTop="16dp"
            android:text="Role"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/profile_picture_layout"
            app:layout_constraintVertical_bias="0.136" app:layout_constraintHorizontal_bias="0.428"/>
    <ImageView
            android:id="@+id/profile_picture_layout"
            android:layout_width="140dp"
            android:layout_height="136dp"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.483"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.099"
            app:srcCompat="@drawable/ic_profile"/>

    <TextView
            android:id="@+id/name_layout"
            android:layout_width="271dp"
            android:layout_height="36dp"
            android:layout_marginTop="16dp"
            android:text="Name"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/profile_picture_layout"
            app:layout_constraintVertical_bias="0.0" app:layout_constraintHorizontal_bias="0.45"/>

    <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email"
            android:textColor="#636363"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"/>

    <TextView
            android:id="@+id/emailTextView"
            android:layout_width="251dp"
            android:layout_height="23dp"
            android:text="TextView"
            android:textColor="#FBFBFB"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.756"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:id="@+id/textView9"
            android:layout_width="105dp"
            android:layout_height="24dp"
            android:text="Phone number"
            android:textColor="#787676"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.205"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.637"/>
    <TextView
            android:id="@+id/phoneNumberTextView"
            android:layout_width="198dp"
            android:layout_height="26dp"
            android:text="TextView"
            android:textColor="#FAF9F9"
            android:textSize="16sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.816"
            app:layout_constraintVertical_bias="0.639"/>
    <Button
            android:text="Edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/edit_button"
            app:srcCompat="@drawable/ic_profile"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintVertical_bias="0.809" android:scrollbarSize="5sp"/>


</androidx.constraintlayout.widget.ConstraintLayout>